{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "../../tsconfig.json",
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "isolatedModules": false,
    "baseUrl": ".",
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "esnext",
    "target": "es6",
    "jsx": "react-jsx"
  },
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ]
}
